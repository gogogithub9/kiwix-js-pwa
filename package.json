{
  "name": "kiwix_js_windows",
  "productName": "Kiwix JS for Windows NWJS Edition",
  "version": "0.9.97N",
  "description": "Kiwix JS Windows for NWJS",
  "main": "index.html",
  "domain": "kiwix.js.windows",
  "window": {
    "width": 1280,
    "height": 840,
    "min_width": 640,
    "min_height": 480,
    "icon": "www/img/icons/kiwix-256.png"
  },
  "scripts": {
    "start": "run --x86 --mirror https://dl.nwjs.io/ .",
    "start-nwjs": "C:\\UTILS\\nwjs-sdk-v0.40.1-win-x64\\nw.exe .",
    "start-nwjs-xp": "C:\\UTILS\\nwjs-sdk-v0.14.7-win-ia32\\nw.exe .",
    "dist-win-x86": "build --tasks win-x86 --mirror https://dl.nwjs.io/ .",
    "dist-win-x64": "build --tasks win-x64 --mirror https://dl.nwjs.io/ ."
  },
  "build-xp": {
    "nwVersion": "0.14.7",
    "output": "bld/nwjs/win-x86-xp",
    "targets": [
      "zip"
    ],
    "files": [
      "pwabuilder-sw.js",
      "index.html",
      "CHANGELOG.md",
      "LICENCE",
      "www/**",
      "archives/README.md",
      "archives/wikip*.*"
    ],
    "win": {
      "icon": "www/img/icons/kiwix-64.ico"
    }
  },
  "build-lts": {
    "nwVersion": "0.39.3",
    "output": "bld/nwjs/win-x86-nwjsv0393",
    "targets": [
      "zip"
    ],
    "files": [
      "pwabuilder-sw.js",
      "index.html",
      "CHANGELOG.md",
      "LICENCE",
      "www/**",
      "archives/README.md",
      "archives/wikip*.*"
    ],
    "win": {
      "icon": "www/img/icons/kiwix-64.ico"
    }
  },
  "build": {
    "nwVersion": "0.44.1",
    "output": "bld/nwjs/win-x86-nwjs0441",
    "targets": [
      "zip"
    ],
    "files": [
      "CHANGELOG.md",
      "LICENCE",
      "archives/README.md",
      "archives/wikip*.*"
    ],
    "win": {
      "icon": "www/img/icons/kiwix-64.ico"
    }
  },
  "repository": "https://github.com/kiwix/kiwix-js-windows",
  "keywords": [
    "Kiwix",
    "offline",
    "Wikipedia",
    "ZIM",
    "reader"
  ],
  "author": "Kiwix",
  "license": "CC0-1.0",
  "devDependencies": {
    "nwjs-builder-phoenix": "^1.15.0"
  }
}
